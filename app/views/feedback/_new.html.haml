= "Leave feedback for #{ @manuscript.title }."
= form_for @feedback, :url => manuscript_feedback_index_path( params[ :id ] ) do | f |
    = render :partial => 'shared/error_messages', :locals => { :object => f.object }
    %p
        = f.text_area :content
    %p
        = f.submit 'Submit'
%p
    = button_to 'Nevermind', '#', :class => 'close-dialog'